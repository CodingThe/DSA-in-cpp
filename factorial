#include <bits/stdc++.h>
using namespace std;
int fact(int n){
    if(n==1){
        return n;
    }
    return n*fact(n-1);
}
int itfact(int n){
    int ans=1;
    for(int i=1;i<=n;i++){
        ans*=i;
    }
    return ans;
}
int main()
{
    int n;
    cin>>n;
    int p=fact(n);
    cout<<p<<endl;
    int w=itfact(n);
    cout<<w<<endl;
    return 0;
}
